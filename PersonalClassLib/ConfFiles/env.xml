<?xml version="1.0" encoding="utf-8" ?>
<variables>
	<!--	configFiles permet de lister l'ensemble des fichiers de configuration et de permettre leur utilisation depuis un même point -->
	<configFiles>
		<var key="FileConfig_DomainActive">
			<value>ConfFiles/domains.xml</value>
		</var>
		<var key="FileConfig_Errors">
			<value>ConfFiles/errors.xml</value>
		</var>
	</configFiles>
	<!-- configVariables correspod aux variables attendues dans l'automate elle contient :
	- 
	-->
	<configVariables>
	
		<!--	G_CONTEXT_PROD => Variable globale des TestsSuite déterminant si l'Application est en développpement ou en Production -->
		<var key="Nom_VariableGlobale_ContextProject" name="G_CONTEXT_PROD">
			<value key="Developpement">
				DEVELOPEMENT
				<default/>
			</value>
			<value key="Production">
				PROD
			</value>
		</var>
	</configVariables>
</variables>
